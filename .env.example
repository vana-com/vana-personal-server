# Server configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Replicate API configuration
REPLICATE_API_TOKEN=your_replicate_api_token_here
REPLICATE_API_BASE_URL=https://api.replicate.com/v1
PERSONAL_SERVER_VERSION=vana-com/personal-server:6dae0fead4017557a2e6bd020643359ac1769228a9cfe3bd2365eeeb9711610e

# Identity service configuration
VANA_MNEMONIC=shoe pass menu sniff phrase despair corn phone then rotate sheriff drop
MNEMONIC_LANGUAGE=english

# Logging configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# CORS configuration
CORS_ORIGINS=*
CORS_METHODS=GET,POST,PUT,DELETE
CORS_HEADERS=*

# API configuration
API_PREFIX=/api/v1
API_TITLE=Vana Personal-Server API
API_VERSION=0.1.0
API_DESCRIPTION=A user-scoped compute service that executes permissioned operations on private data.

# Settings validation
VALIDATE_SETTINGS_ON_IMPORT=true

# Upstash Redis Configuration
UPSTASH_REDIS_REST_URL=https://upstash-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=upstash-token-here

# Rate Limiting Configuration
RATE_LIMIT_ENABLED=true
RATE_LIMIT_USE_IP=true
RATE_LIMIT_USE_SIGNATURE=true

# Limits
RATE_LIMIT_DEFAULT_RPM=60              # Default requests per minute
RATE_LIMIT_OPERATIONS_PER_HOUR=100     # Operations per hour
RATE_LIMIT_IDENTITY_RPM=20             # Identity requests per minute

# Whitelist (comma-separated)
RATE_LIMIT_WHITELIST_IPS=